# API Key for securing the endpoint
API_KEY="your-secret-api-key"

# Cache Time-To-Live (TTL) in seconds
CACHE_TTL_GAMES=900       # TTL for club/team game lists
CACHE_TTL_TABLE=3600      # TTL for team league tables
CACHE_TTL_TEAMS=7200      # TTL for club team lists
CACHE_TTL_FONT=86400      # TTL for score deobfuscation font maps

# Log level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL="INFO"

# Proactive Caching (Pre-warming)
# Set the club ID to enable pre-warming for a specific club
PREWARM_CLUB_ID="001VTR8D8C000000VARTQG41VT4929AS"
PREWARM_INTERVAL_SECONDS=300

# Cache directory (inside container or on host)
# Default: ./cache
# Example for Docker: /app/cache
CACHE_DIR="./cache"
